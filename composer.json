	"description": "Show the number of topics the user started",
	"homepage": "https://github.com/marttiphpbb/phpbb-ext-usertopiccount",
	"version": "0.6.1",
	"time": "2020-05-05",
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "martti",
			"email": "info@martti.be",
			"homepage": "https://github.com/marttiphpbb/",
			"role": "Developer"
		}],
	"require": {
		"php": ">=7.1"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
